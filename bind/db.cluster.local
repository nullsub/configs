;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cluster.local. root.cluster.local. (
			      4		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.cluster.local.
@	IN	A	192.168.2.1
ns	IN	A	192.168.2.1
headnode	IN	A	192.168.2.1
node0	IN	A	192.168.2.10
node1	IN	A	192.168.2.11
