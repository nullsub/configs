;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cluster.local. root.cluster.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.cluster.local.
1	IN	PTR	headnode.cluster.local.
2	IN	PTR	node0.cluster.local.
3	IN	PTR	node1.cluster.local.
