#!/usr/bin/gnuplot
reset

set terminal png size 1000,800

set title "MPI alltoall time"
set ylabel "usec"
set xlabel "Message Size in Byte"
set format x '%.0s%cB'
set key reverse Left outside
set grid
set style data linespoints
set logscale x 2
set logscale y 2

plot "mpi_alltoall" using 1:5 title "",
