set terminal png size 900,900
set title "MPI Speedup"
set ylabel "Speedup"
set xlabel "Cores"
set grid
set key font ",25"
set key spacing 2.7

f(x) = x
plot 	f(x) title 'ideal speedup' with lines linestyle 1, \
	"birch1.txt" using 1:3 with linespoints lw 2 lt 3 title 'birch1, 100.000 Points',\
	"s1.txt" using 1:3 with linespoints lw 2 lt 2 title 's1, 5.000 Points'
