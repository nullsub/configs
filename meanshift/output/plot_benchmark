set terminal png size 1000,1000
set title "Meanshift Dauer"
set ylabel "Dauer gesamt"
set xlabel "Cores"
set ydata time
set timefmt "%H:%M:%S"
set format y "%H:%M:%S"
set grid
set xrange ["1":"300"]
set logscale y 2
set logscale x 2
plot    "birch1.txt" using 1:2  with lines lw 2 lt 3 title 'birch1, 100000 Points', \
	"s1.txt" using 1:2 with lines lw 2 lt 2 title 's1, 5000 Points', \
