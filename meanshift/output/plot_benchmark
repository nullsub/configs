set terminal png size 900,900
set title "Meanshift Laufzeit"
set ylabel "Laufzeit gesamt"
set xlabel "Cores"
set ydata time
set timefmt "%H:%M:%S"
set format y "%H:%M:%S"
set grid
set key font ",25"
set key spacing 2.7
set xrange ["0.9":"300"]
set logscale y 2
set logscale x 2
plot    "birch1.txt" using 1:2  with linespoints lw 2 lt 3 title 'birch1, 100.000 Points', \
	"s1.txt" using 1:2 with linespoints lw 2 lt 2 title 's1, 5.000 Points', \
