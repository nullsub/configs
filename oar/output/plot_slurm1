set terminal png size 1920,1080
set output 'pics/slurm1.png'

#set format x "%d"
set xlabel "queuesize"
set yrange [0:10000]

set ylabel "time to submit one job in ms"
set yrange [0:1000]

set title "Slurm job submission"
set key reverse Left outside
set grid

set style data linespoints

plot "../data/sm1.txt" using 1:2 every 150


